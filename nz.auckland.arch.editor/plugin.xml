<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated arch -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="nz.auckland.arch.presentation.ArchModelWizardID"
            name="%_UI_ArchModelWizard_label"
            class="nz.auckland.arch.presentation.ArchModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/ArchModelFile.gif">
         <description>%_UI_ArchModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated arch -->
      <editor
            id="nz.auckland.arch.presentation.ArchEditorID"
            name="%_UI_ArchEditor_label"
            icon="icons/full/obj16/ArchModelFile.gif"
            extensions="arch"
            class="nz.auckland.arch.presentation.ArchEditor"
            contributorClass="nz.auckland.arch.presentation.ArchActionBarContributor">
      </editor>
   </extension>

</plugin>
